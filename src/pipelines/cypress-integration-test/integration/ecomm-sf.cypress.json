{
    "baseUrl": "#{ECOMM_PORTAL_BASE_URL}#",
    "username": "#{ECOMM_USERNAME}#",
    "password": "#{ECOMM_PASSWORD}#",
    "campaignReceiver": "#{ECOMM_CAMPAIGN_RECEIVER}#",
    "env": {
      "runtimeEnv": "#{RUNTIME_ENV}#",
      "storageAccountConnString": "#{STORAGE_ACCOUNT_CONN_STRING}#"      
    },
    "defaultCategory": "Cypress Trees",
    "defaultCategoryUrl": "cypress",
    "defaultLanguage": "English",
    "chromeWebSecurity": false,
    "defaultCommandTimeout": 120000,
    "supportFile": "cypress/support/index.ts",
    "reporter": "cypress-mochawesome-reporter",
    "reporterOptions": {
      "reportDir": "/e2e/cypress/report",
      "charts": true,
      "reportPageTitle": "ecomm-sf",
      "embeddedScreenshots": false
    },
    "video": false,
    "screenshotsFolder": "/e2e/cypress/screenshots",
    "retries": 2   
}