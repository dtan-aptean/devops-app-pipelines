{
    "baseUrl": "#{ECOMM_API_SVC_BASE_URL}#",
    "env": {
        "x-aptean-apim": "#{ECOMM_APIM}#",
        "x-aptean-tenant": "#{ECOMM_TENANT_ID}#",
        "x-aptean-tenant-secret": "#{ECOMM_TENANT_SECRET}#",
        "gqlTimeout": 5000,
        "storefrontUrl": "#{ECOMM_PORTAL_BASE_URL}#",
        "storefrontLogin": "#{ECOMM_USERNAME}#",
        "storefrontPassword": "#{ECOMM_PASSWORD}#"      
    },
    "chromeWebSecurity": false,
    "defaultCommandTimeout": 120000,
    "supportFile": "cypress/support/index.ts",
    "reporter": "junit",
    "reporterOptions": {
      "mochaFile": "/e2e/cypress/results/test-result-ecomm-api-svc-[hash].xml"
    },
    "videosFolder": "/e2e/cypress/videos",
    "screenshotsFolder": "/e2e/cypress/screenshots",
    "retries": 2   
}